.new-action
  position: fixed;
  width: 56px
  height: 56px
  border-radius: 50%
  bottom: 15px;
  right: 15px;
  z-index: 1;
  opacity: 1
  background-color: darkblue




.plus-icon-div
  position: absolute;
  top: 0
  width: 56px;
  height: 56px;
  display: flex;
  justify-content: center;
  z-index: -1


.plus-icon
  align-self: center;
  color: white
  cursor: pointer;

.organizations-new-div
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  bottom: 0;
  background: white;
  overflow: scroll;
