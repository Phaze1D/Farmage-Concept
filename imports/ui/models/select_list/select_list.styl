.list
  height: 100%
  width: 100%;
  padding-top: 64px
  background-color: #FAFAFA
  overflow-y: scroll;
  overflow-x: hidden;

  @media (max-width: 768px){
    padding-top: 56px
  }

  & .header
    position: absolute;
    z-index: 2
    top: 0
    left: 0
    height: 64px
    width: 100%
    background-color: white

    @media (max-width: 768px){
      height: 56px
    }

    & .title-div
      display: inline-flex;
      height: 100%
      position: absolute;
      z-index: -1

      & .title
        font-size: 18px
        font-weight: 300
        align-self: center;



    & .search-bar
      // width: 40%
      height: 100%
      display: flex;
      float: right;
      justify-content: flex-end;
      background-color: white

      & .search-input-div
        position: relative;
        overflow: hidden;
        width: 40%
        display: flex;
        justify-content: center;

        @media(max-width: 424px){
          max-width: 200px
        }

        @media(max-width: 374px){
          max-width: 150px
        }

        & .search-input
          width: 100%
          align-self: center;

          & .input-container
            padding-right: 24px

        & .cross-div
          display: flex;
          position: absolute;
          height: 100%
          right: 0
          top: 0
          z-index: 2
          cursor: pointer;


          & i
            font-size: 22px
            align-self: center;
            background-color: white


  & .b-wrap
    width: 54px
    height: calc(100% - 10px)
    border-radius: 40px
    margin: 5px 5px
    display: flex;
    justify-content: center;

    @media (max-width: 768px){
      width: 46px

    }

    & i
      align-self: center;



  & .items
    background-color: #FAFAFA
